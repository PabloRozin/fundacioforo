body
	padding 0
	margin 0
	padding-top header-small-height
	transition padding-top .5s

	@media screen and (min-width: width-break)
		padding-top header-big-height
		
		&.scrolled
			padding-top 90px
			
	.wrapper
		overflow-x hidden

	.delay-1
		animation-delay: .5s;

		&.fast
			animation-delay: .3s

	.delay-2
		animation-delay: 1s;

		&.fast
			animation-delay: .6s

	.delay-3
		animation-delay: 1.5s;

		&.fast
			animation-delay: .9s

	.delay-4
		animation-delay: 2s;

		&.fast
			animation-delay: 1.2s

	.delay-5
		animation-delay: 2.5s;

		&.fast
			animation-delay: 1.5s
	
	.full_size
		
		.vertical_align
			position: absolute
			width 100%
			
	.hide-on-celphone
		
		@media screen and (max-width: width-break)
			display none
			
	.popup
		position: fixed
		top 0
		left -100%
		z-index 1000
		
		&.open
			left 0
			right 0
		
		.vertical_align
			z-index 2
			
		.hc-title
			padding 0 0 15px 0 !important
			
		.popup-content
				
			.popup-cont
				position: relative
				max-width 350px
				margin 0 auto
				
				.popup-body
					background white
					padding 30px 30px 15px 30px
					position relative
					box-sizing border-box
					display: none
					
					&.open
						display block
			
		.popup-close
			position: absolute
			top 30px
			right 30px
			cursor pointer
			color color-verde
			z-index 1
			
			&:hover 
				color color-verde-oscuro
		
		&:before
			position absolute
			top 0
			left 0
			right 0
			bottom 0
			content ""
			z-index 1
			background white
			
			@media screen and (min-width: width-break)
				background black
				opacity .7

	.table
		display: table;
		width: 100%
		height: 100%
		vertical-align: middle;

		.table-cell
			display: table-cell;
			height: 100%
			width: 100%
			vertical-align: middle;

	*
		font-family font-family
		margin 0
		padding 0
		border 0
		text-decoration: none
		transition: color .5s, background-color .5s, border-color .5s, text-shadow .5s, opacity .5s

		&:focus
			outline none

	.center
		max-width max-width
		padding 0 15px
		margin 0 auto
		box-sizing border-box
		position relative

	.clearfix:before
	.clearfix:after
		content " "
		display table

	.clearfix:after
		clear both

	.clearfix
		*zoom 1

	.row
		margin 0 (-(col-padding / 2))
		padding-bottom col-padding
		vertical-align bottom
		*zoom 1

		&.no-padding
			margin 0
			padding 0

			.col
				padding 0
				margin 0

		&:before
		&:after
			content " "
			display table

		&:after
			clear both

		.col
			padding 0 (col-padding / 2)
			display inline-block
			box-sizing border-box
			width 100%
			margin-top col-padding
			vertical-align bottom
			
			&.col-1-12
				width 8.3333%

			@media screen and (min-width: 500px)

				&.col-1-4
				&.col-1-6
				&.col-1-5
					width: 50%

			@media screen and (min-width: 700px)

				&.col-1-2
					width 50%
			
				&.col-1-6
				&.col-1-5
					width: 33.3333%

			@media screen and (min-width: 900px)
					
				&.col-1-4
					width: 25%

				&.col-1-5
					width: 20%

				&.col-1-6
					width: 16.6666%

	.only-desktop
		display: none

		@media screen and (min-width: width-break)
			display: block

	.only-mobile

		@media screen and (min-width: width-break)
			display: none

	.btn
		font-size button-font-size - 4
		line-height button-font-size
		font-weight button-weight
		height button-height
		padding ((button-height - (button-font-size + 4) ) / 2) (button-height / 2)
		cursor pointer
		display inline-block
		color button-text-color
		text-align center
		box-sizing border-box
		background: button-bg-color
		letter-spacing button-letter-spacing
		border-radius button-border-radius
		
		@media screen and (min-width: width-break)
			font-size button-font-size
			
		&:hover
			color button-text-hover-color
			background: button-bg-hover-color
			
		&.btn-100
			width 100%
			box-sizing border-box
			text-align center

		&.btn-big
			font-size button-big-font-size
			line-height button-big-font-size + 4
			font-weight button-big-weight
			height button-big-height
			padding ((button-big-height - (button-big-font-size + 4) ) / 2) (button-big-height / 2)
			letter-spacing button-big-letter-spacing
		
			@media screen and (min-width: width-break)
				font-size button-big-font-size

		&.btn-small
			font-size button-small-font-size
			line-height button-small-font-size + 4
			font-weight button-big-weight
			height button-small-height
			padding ((button-small-height - (button-small-font-size + 4) ) / 2) (button-small-height / 2)
			letter-spacing button-small-letter-spacing
		
			@media screen and (min-width: width-break)
				font-size button-small-font-size
				
		&.btn-secondary
			background button-text-color
			color button-bg-color
			border solid 1px button-bg-color
			
			&:hover
				color button-text-hover-color
				background: button-bg-hover-color
				border-color: button-bg-hover-color

	.form

		.message
			display none

			.title
				font-size form-input-font-size - 4
				line-height form-input-font-size + 4
				margin-bottom (col-padding / 2)
			
				@media screen and (min-width: width-break)
					font-size form-input-font-size
				
		.options
			padding-top 10px
			
			.option
				font-size form-input-font-size - 4
				line-height form-input-font-size + 4
				margin-top 5px
			
				@media screen and (min-width: width-break)
					font-size form-input-font-size
				
				a
					color button-bg-color
					
					&:hover
						color button-bg-hover-color
					

		.item
			margin-bottom (col-padding / 2)
			
			&.radio
			&.checkbox
				
				.title
					padding-bottom 15px

			&.has-error
				
				select
				input[type="text"]
				input[type="email"]
				input[type="password"]
				input[type="number"]
				input[type="date"]
				textarea
					border-color red
					
			.radio_options
				margin-top -15px
				
				.radio_option
					display block
					margin-top 15px
					position: relative
					padding-left 25px
					
					input[type="radio"]
					input[type="checkbox"]
						position absolute
						left 0
						top 0px
						
					textarea
						margin-top 5px
						max-width 300px
						display: block

			.checkbox

				label
					display block
					font-size form-input-font-size - 4
					line-height form-input-font-size + 4
					font-weight form-input-weight
					color: form-input-text-color
					letter-spacing form-input-letter-spacing
					cursor pointer
			
					@media screen and (min-width: width-break)
						font-size form-input-font-size
					
					input
						margin-right 5px

			select
			input[type="text"]
			input[type="email"]
			input[type="password"]
			input[type="number"]
			input[type="date"]
			textarea
				box-sizing border-box
				width 100%
				border solid 1px form-input-border-color
				font-size form-input-font-size - 4
				line-height form-input-font-size + 4
				font-weight form-input-weight
				height form-input-height
				padding ((form-input-height - (form-input-font-size + 4) ) / 2) (button-height / 2)
				background: form-input-bg-color
				color: form-input-text-color
				letter-spacing form-input-letter-spacing
				border-radius form-border-radius
			
				@media screen and (min-width: width-break)
					font-size form-input-font-size

			textarea
				resize none
				height form-textarea-height

			&.last
				margin-bottom: 0

	.owl-theme

		.owl-dots

			.owl-dot

				span
					background bg-color
					border-radius 0
					margin-bottom 10px
					width 15px
					height 15px
					transition all .3s

				&:hover
				&.active

					span
						background bg-color-dark

		.owl-controls
			margin-top: 0

			.owl-nav
				position absolute
				top 50%
				margin-top -40px
				width 1140px
				left 50%
				margin-left -570px

				.owl-prev
				.owl-next
					width auto
					line-height 60px
					height 60px
					border-radius 0
					background none
					padding 0
					color white
					margin 0
					position absolute
					transition all .3s

					&:hover
						background none

					&:before
						content " "
						display inline-block
						font-family 'icomoon'
						font-size 67px

				.owl-prev
					left -90px

					&:before
						content "\e909"

				.owl-next
					right -90px

					&:before
						content "\e903"

#noty_top_layout_container{top:0 !important;width:100% !important;left:0 !important;}
#noty_top_layout_container .heniaxTheme .noty_bar .noty_message{text-align:center;padding:13px 15px;}
#noty_top_layout_container .heniaxTheme .noty_bar .noty_message .noty_text{color:#fff !important;font-size:16px;line-height:21px}
#noty_top_layout_container .heniaxTheme .noty_bar.noty_type_success{background:#5cb85c}
#noty_top_layout_container .heniaxTheme .noty_bar.noty_type_error{background:#cf3535}

.dropzone
	border dashed 2px color-gris-borde
	font-size form-input-font-size - 4
	line-height form-input-font-size + 4
	font-weight form-input-weight
	padding 15px
	
	label
		display none
		
	.dz-message
		margin 0
		line-height 150px
		
	.dz-preview
		
		&.dz-file-preview 
			
			.dz-image
				background: form-input-bg-color
				border-radius form-border-radius
				border solid 1px color-gris-borde
			
		.dz-image
			background: form-input-bg-color
			border-radius form-border-radius
				